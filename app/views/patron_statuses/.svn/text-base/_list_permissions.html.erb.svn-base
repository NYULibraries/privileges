    <% webtext = if @sublibrary.web_text.empty? then webtext = @sublibrary.code else webtext = @sublibrary.web_text end %>
	<div id="change"></div>
	<div id="permission_chart">
	<h3><%= webtext %></h3>
        <div id="permissions_<%=@sublibrary.code%>" class="permissions_list">
		<% remote_form_for @patron_status_permission, :url => {:controller=>"patron_status_permissions",:action=>"create_remote"} do |f| %>
		<%= f.error_messages %>
		<%= f.hidden_field :patron_status_code, :value => @patron_status.code %>
		<%= f.hidden_field :sublibrary_code, :value => @sublibrary.code %>
		<%= f.hidden_field :fromAleph, :value => 0 %>
		<table>
			<thead>
			<tr>
				<th>Permission</th>
				<th>Value</th>
			</tr>
			</thead>
			<tbody>
			<%= print_patron_status_permissions%> 
        	<tr>
        	<%= render :partial => "patron_status_permissions/new", :locals => { :f => f }%>
        	</tr>
			</tbody>
		</table>
		<% end %>
		</div>
		</div>
