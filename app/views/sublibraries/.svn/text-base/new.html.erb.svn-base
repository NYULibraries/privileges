<h1>New sublibrary</h1>

<% form_for(@sublibrary) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :code %><br />
    <span class="grayed">nyu_ag_noaleph_</span> <%= f.text_field :code %>
  </p>
  <!--p>
    <%= f.label :original_text %><br />
    <%= f.text_field :original_text %>
  </p-->
  <p>
    <%= f.label :web_text %><br />
    <%= f.text_field :web_text %>
  </p>
 	<p>
    <%= f.label :under_header, "Grouping" %><br />
    <%= f.text_field :under_header %>
  </p>
    <%= f.hidden_field :fromAleph, :value => 0 %>
	<%= f.hidden_field :prefix, :value => "nyu_ag_noaleph_"%>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', sublibraries_path %>
